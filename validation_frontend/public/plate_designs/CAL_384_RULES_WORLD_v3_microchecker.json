      "contrastive_tiles.tiles[].assignment",
      "biological_anchors.wells",
      "non_biological_provocations.*.wells",
      "non_biological_provocations.cell_density_gradient.rule",
      "global_defaults.default_assignment"
    ],
    "derived_fields": {
      "cell_line": "from cell_lines.well_to_cell_line",
      "cell_density": "from density gradient by column unless explicitly set",
      "stain_scale": "1.0 unless in STAIN_LOW or STAIN_HIGH wells",
      "fixation_timing_offset_min": "0 unless in EARLY_FIX or LATE_FIX wells",
      "imaging_focus_offset_um": "0 unless in FOCUS_MINUS or FOCUS_PLUS wells"
    }
  },
  "expected_learning_deliverables": [
    "Spatial correction model that generalizes across both cell lines (row/col/edge plus smooth 2D surface)",
    "Noise floor per feature family under VEHICLE at LOW/NOMINAL/HIGH density",
    "Quantified sensitivity to stain scaling (0.9x vs 1.1x) and whether normalization is distorting manifolds",
    "Quantified sensitivity to fixation timing jitter and focus drift",
    "LDH vs morphology coupling check: baseline independence and saturation behavior under ANCHOR_DEATH",
    "Outlier and failure-mode catalog (segmentation failures, empty wells, illumination artifacts, edge evaporation signatures)"
  ],
  "sanity_checks": [
    {
      "name": "cell_line_invariance_to_plate_model",
      "description": "After spatial correction, the plate-effect model should not need separate parameters per cell line to remove gross drift."
    },
    {
      "name": "density_axis_detectable",
      "description": "Vehicle morphology should show a consistent density axis; if not, either density modeling is wrong or features are over-normalized."
    },
    {
      "name": "stain_scale_axis_detectable",
      "description": "STAIN_LOW vs STAIN_HIGH should be separable primarily in intensity-like features; if it rotates the entire manifold, normalization is broken."
    },
    {
      "name": "focus_probes_break_things_in_a_known_way",
      "description": "FOCUS_MINUS/PLUS should degrade sharpness/texture features; if it changes everything equally, feature extraction is conflating signals."
    },
    {
      "name": "anchors_are_orthogonal",
      "description": "ANCHOR_MORPH and ANCHOR_DEATH should not collapse onto the same direction; if they do, you are mostly measuring generic sickness."
    },
    {
      "name": "no_cell_background_stable",
      "description": "NO_CELLS wells should cluster tightly; if not, staining/background variability is large enough to masquerade as phenotype."
    }
  ]
}